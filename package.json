{
  "name": "logalize",
  "version": "0.0.1",
  "description": "A better window.console",
  "keywords": [
    "browser",
    "colors",
    "console",
    "debug",
    "devtools",
    "developer",
    "wrapper"
  ],
  "main": "src/index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "webpack -p && node-sass index.scss dist/logalize.css",
    "lint": "eslint src/*.js test/*.js",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch"
  },
  "author": "Alexander Komarov <ak@akxcv.com> (http://akxcv.com)",
  "license": "MIT",
  "repository": "akxcv/logalize",
  "dependencies": {},
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.3.2",
    "babel-preset-env": "^1.1.8",
    "babel-preset-es2015": "^6.22.0",
    "eslint": "^3.15.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-es5": "^1.0.1",
    "eslint-plugin-promise": "^3.4.1",
    "eslint-plugin-standard": "^2.0.1",
    "jest": "^18.1.0",
    "jest-cli": "^18.1.0",
    "node-sass": "^4.5.0",
    "rimraf": "^2.6.0",
    "uglifyjs": "^2.4.10",
    "webpack": "^2.2.1"
  },
  "eslintConfig": {
    "extends": "eslint-config-standard",
    "rules": {
      "no-multi-spaces": 0
    },
    "env": {
      "browser": true,
      "jest": true
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "**/src/*.js",
      "!**/src/app.js"
    ],
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    }
  }
}
